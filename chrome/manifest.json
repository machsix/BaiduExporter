{
  "applications": {
	"gecko": {
	  "id":"baiduexporter@machsix.github.com",
	  "strict_min_version":"52.0",
	  "update_url":"https://machx.github.io/update.json"
    }
  },
  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": ["js/start.js" ],
      "css": [ "css/style.css" ],
      "matches": [ "*://pan.baidu.com/s/*", "*://yun.baidu.com/s/*", "*://pan.baidu.com/disk/home*", "*://yun.baidu.com/share/link*", "*://pan.baidu.com/share/link*", "*://yun.baidu.com/disk/home*", "*://pan.baidu.com/pcloud/album/*", "*://yun.baidu.com/pcloud/album/*" ],
      "run_at": "document_idle"
    }
  ],
  "default_locale": "en",
  "icons": {
    "32": "img/logo32.png",
    "16": "img/logo16.png",
    "48": "img/logo48.png",
    "128": "img/logo128.png"
  },
  "web_accessible_resources": [ "js/baidu.js" ],
  "manifest_version": 2,
  "permissions": [ "cookies", "notifications", "*://pan.baidu.com/", "*://yun.baidu.com/", "*://pcs.baidu.com/", "activeTab", "contextMenus", "storage" ],
  "name": "BaiduExporter",
  "description": "Assistant to export download links to aria2/aria2-rpc",
  "author": "acgotaku",
  "version": "1.0.2.10"
}
